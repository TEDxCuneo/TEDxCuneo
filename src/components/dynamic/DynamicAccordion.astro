---
import Accordion from "@components/Accordion.astro";
import DynamicComponent from "@components/DynamicComponent.astro";
import type { TypeComponentAccordionSkeleton } from "@lib/generated/contentful";
import type { Entry } from "@lib/types";
interface Props extends Entry<TypeComponentAccordionSkeleton> {}

const accordion = Astro.props;
---

<Accordion title={accordion.fields.title}>
  <DynamicComponent {...accordion.fields.content!} />
</Accordion>
