---
import type { Entry } from "@lib/types";
import PageSection from "./PageSection.astro";
import DynamicComponent from "./DynamicComponent.astro";

interface Props {
  components?: (Entry<any> | undefined)[];
}

const { components } = Astro.props;
---

{
  components
    ?.filter((c) => c)
    .map((c) => (
      <PageSection>
        <DynamicComponent {...c!} />
      </PageSection>
    ))
}
