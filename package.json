{
	"name": "tedxcuneo",
	"type": "module",
	"version": "2.0.2",
	"scripts": {
		"prepare": "husky",
		"cf-codegen": "eval $(grep '^CONTENTFUL_SPACE_ID' .env) && eval $(grep '^CONTENTFUL_MGMT_TOKEN' .env) && cf-content-types-generator -s ${CONTENTFUL_SPACE_ID} -t ${CONTENTFUL_MGMT_TOKEN} --typeguard -o src/lib/generated/contentful -p  --v10 --jsdoc --preserve",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"clean": "git clean --interactive -dx --exclude .env.example --exclude \".env.*.example\" --exclude node_modules/ --exclude .husky/",
		"lint": "npm-run-all --parallel lint:astro lint:js+ts lint:format lint:types",
		"lint:astro": "astro check",
		"lint:js+ts": "eslint --ext .astro,.js,.jsx,.cjs,.ts,.tsx,.cts --ignore-path .gitignore .",
		"lint:format": "prettier --check src/**",
		"lint:types": "tsc --noEmit",
		"lint:fix": "npm-run-all --serial lint:fix:*",
		"lint:fix:js+ts": "npm run lint:js+ts --fix",
		"lint:fix:format": "prettier --write ."
	},
	"lint-staged": {
		"*.{js,ts,json,md,astro}": [
			"astro check",
			"eslint --fix",
			"prettier  --ignore-unknown --write"
		]
	},
	"dependencies": {
		"@astrojs/check": "^0.5.9",
		"@astrojs/react": "^4.2.2",
		"@astrojs/sitemap": "^3.1.1",
		"@astrojs/tailwind": "^5.1.0",
		"@contentful/rich-text-html-renderer": "^16.3.5",
		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"@types/react": "^19.0.12",
		"@types/react-dom": "^19.0.4",
		"astro": "^4.5.8",
		"astro-robots-txt": "^1.0.0",
		"cf-content-types-generator": "^2.15.0",
		"contentful": "^10.8.4",
		"motion": "^12.6.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"swiper": "^11.0.7",
		"tailwindcss": "^3.4.1"
	},
	"devDependencies": {
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"chalk": "^5.3.0",
		"eslint": "8.57.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "18.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-astro": "^0.32.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-regexp": "2.3.0",
		"eslint-plugin-simple-import-sort": "12.0.0",
		"eslint-plugin-tailwindcss": "3.15.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"nano-staged": "^0.8.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"sass": "^1.72.0",
		"typescript": "^5.4.3"
	}
}
